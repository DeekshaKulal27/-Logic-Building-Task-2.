    "# Problem 7: Count Character Frequency\n",
    "\n",
    "text = \"python\"\n",
    "\n",
    "char_count = {}\n",
    "\n",
    "# Loop through each character\n",
    "for char in text:\n",
    "    if char in char_count:\n",
    "        char_count[char] += 1\n",
    "    else:\n",
    "        char_count[char] = 1\n",
    "\n",
    "print(char_count)\n"
