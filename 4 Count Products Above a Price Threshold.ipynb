   "source": [
    "# Problem 4: Count Products Above a Price Threshold\n",
    "\n",
    "prices = [450, 1200, 899, 1500, 300]\n",
    "\n",
    "count = 0\n",
    "\n",
    "# Loop through prices\n",
    "for price in prices:\n",
    "    if price > 1000:\n",
    "        count += 1\n",
    "\n",
    "print(\"Products above 1000:\", count)\n"
   ]
