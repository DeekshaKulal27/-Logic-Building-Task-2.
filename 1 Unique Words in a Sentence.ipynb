    "# Problem 1: Unique Words in a Sentence\n",
    "\n",
    "sentence = input(\"Enter sentence: \")\n",
    "words = sentence.split()\n",
    "unique_words = set(words)\n",
    "\n",
    "# Printing results\n",
    "print(\"Unique words count:\", len(unique_words))\n",
    "print(\"Unique words:\", unique_words)\n"
